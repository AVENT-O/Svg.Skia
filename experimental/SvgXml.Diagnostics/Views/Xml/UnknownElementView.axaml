<UserControl x:Class="SvgXml.Diagnostics.Views.Xml.UnknownElementView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <Expander IsExpanded="True" Header="Unknown">
            <Grid RowDefinitions="Auto">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" SharedSizeGroup="KeyColumn"/>
                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueColumn"/>
                </Grid.ColumnDefinitions>
                <!-- Tag -->
                <TextBlock Text="Tag" Grid.Row="0" Grid.Column="0" />
                <TextBox Text="{Binding Tag}" Grid.Row="0" Grid.Column="1" />
            </Grid>
        </Expander>
    </StackPanel>
</UserControl>
