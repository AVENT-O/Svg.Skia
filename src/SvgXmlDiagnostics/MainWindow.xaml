<Window x:Class="SvgXmlDiagnostics.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SvgXmlDiagnostics"
        xmlns:Xml="clr-namespace:Xml;assembly=SvgXml"
        xmlns:Svg="clr-namespace:Svg;assembly=SvgXml"
        xmlns:SvgFilterEffects="clr-namespace:Svg.FilterEffects;assembly=SvgXml"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen" WindowState="Normal"
        UseLayoutRounding="True" SnapsToDevicePixels="True" TextOptions.TextRenderingMode="ClearType"
        Title="SvgXml Diagnostics" Height="650" Width="1200">
    <Window.Resources>
        <local:TypeVisibilityConverter x:Key="TypeVisibilityConverter" />
    </Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0" Background="Transparent">
            <MenuItem Header="_File">
                <MenuItem Name="FileOpen" Header="_Open" Click="FileOpen_Click"/>
                <Separator/>
                <MenuItem Name="FileExit" Header="E_xit" Click="FileExit_Click"/>
            </MenuItem>
        </Menu>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="300"/>
                <ColumnDefinition Width="4"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <TreeView x:Name="DocumentTree" VirtualizingPanel.VirtualizationMode="Recycling" Grid.Column="0">
                <TreeView.Resources>
                    <Style TargetType="{x:Type TreeViewItem}">
                        <Setter Property="IsExpanded" Value="True"/>
                    </Style>
                </TreeView.Resources>
                <TreeView.ItemTemplate>
                    <HierarchicalDataTemplate ItemsSource="{Binding Children}">
                        <TextBlock Text="{Binding}" />
                    </HierarchicalDataTemplate>
                </TreeView.ItemTemplate>
            </TreeView>
            <GridSplitter Grid.Column="1" ResizeDirection="Columns" ResizeBehavior="PreviousAndNext"/>
            <TabControl DataContext="{Binding ElementName=DocumentTree, Path=SelectedItem, UpdateSourceTrigger=PropertyChanged}" Grid.Column="2">
                <TabItem Header="Element" Visibility="Visible">
                    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
                        <ContentControl Content="{Binding}">
                            <ContentControl.Resources>
                                <!-- Xml -->
                                <DataTemplate DataType="{x:Type Xml:ContentElement}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- Content -->
                                        <TextBlock Text="Content" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Content}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Xml:UnknownElement}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- Tag -->
                                        <TextBlock Text="Tag" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Tag}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <!-- Animation -->
                                <DataTemplate DataType="{x:Type Svg:SvgAnimate}">
                                    <Grid Margin="2">
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgAnimateColor}">
                                    <Grid Margin="2">
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgAnimateMotion}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- Path -->
                                        <TextBlock Text="Path" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Path}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- KeyPoints -->
                                        <TextBlock Text="KeyPoints" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding KeyPoints}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- Rotate -->
                                        <TextBlock Text="Rotate" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Rotate}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                        <!-- Origin -->
                                        <TextBlock Text="Origin" Grid.Row="3" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Origin}" Grid.Row="3" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgAnimateTransform}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- Type -->
                                        <TextBlock Text="Type" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Type}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgMotionPath}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- Href -->
                                        <TextBlock Text="Href" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Href}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgSet}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- To -->
                                        <TextBlock Text="To" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding To}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <!-- Basic Shapes -->
                                <DataTemplate DataType="{x:Type Svg:SvgCircle}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- CenterX -->
                                        <TextBlock Text="CenterX" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding CenterX}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- CenterY -->
                                        <TextBlock Text="CenterY" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding CenterY}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- Radius -->
                                        <TextBlock Text="Radius" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Radius}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgEllipse}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- CenterX -->
                                        <TextBlock Text="CenterX" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding CenterX}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- CenterY -->
                                        <TextBlock Text="CenterY" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding CenterY}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- RadiusX -->
                                        <TextBlock Text="RadiusX" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding RadiusX}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                        <!-- RadiusY -->
                                        <TextBlock Text="RadiusY" Grid.Row="3" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding RadiusY}" Grid.Row="3" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgLine}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- StartX -->
                                        <TextBlock Text="StartX" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding StartX}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- StartY -->
                                        <TextBlock Text="StartY" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding StartY}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- EndX -->
                                        <TextBlock Text="EndX" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding EndX}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                        <!-- EndY -->
                                        <TextBlock Text="EndY" Grid.Row="3" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding EndY}" Grid.Row="3" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgPolygon}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- Points -->
                                        <TextBlock Text="Points" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Points}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgPolyline}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- Points -->
                                        <TextBlock Text="Points" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Points}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgRectangle}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- X -->
                                        <TextBlock Text="X" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding X}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- Y -->
                                        <TextBlock Text="Y" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Y}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- Width -->
                                        <TextBlock Text="Width" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Width}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                        <!-- Height -->
                                        <TextBlock Text="Height" Grid.Row="3" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Height}" Grid.Row="3" Grid.Column="1" Margin="2" />
                                        <!-- CornerRadiusX -->
                                        <TextBlock Text="CornerRadiusX" Grid.Row="4" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding CornerRadiusX}" Grid.Row="4" Grid.Column="1" Margin="2" />
                                        <!-- CornerRadiusY -->
                                        <TextBlock Text="CornerRadiusY" Grid.Row="5" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding CornerRadiusY}" Grid.Row="5" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <!-- Clipping and Masking -->
                                <DataTemplate DataType="{x:Type Svg:SvgClipPath}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- ClipPathUnits -->
                                        <TextBlock Text="ClipPathUnits" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding ClipPathUnits}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgMask}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- X -->
                                        <TextBlock Text="X" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding X}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- Y -->
                                        <TextBlock Text="Y" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Y}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- Width -->
                                        <TextBlock Text="Width" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Width}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                        <!-- Height -->
                                        <TextBlock Text="Height" Grid.Row="3" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Height}" Grid.Row="3" Grid.Column="1" Margin="2" />
                                        <!-- MaskUnits -->
                                        <TextBlock Text="MaskUnits" Grid.Row="4" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding MaskUnits}" Grid.Row="4" Grid.Column="1" Margin="2" />
                                        <!-- MaskContentUnits -->
                                        <TextBlock Text="MaskContentUnits" Grid.Row="5" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding MaskContentUnits}" Grid.Row="5" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <!-- Color -->
                                <DataTemplate DataType="{x:Type Svg:SvgColorProfile}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- Local -->
                                        <TextBlock Text="Local" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Local}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- Name -->
                                        <TextBlock Text="Name" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Name}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- RenderingIntent -->
                                        <TextBlock Text="RenderingIntent" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding RenderingIntent}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                        <!-- Href -->
                                        <TextBlock Text="Href" Grid.Row="3" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Href}" Grid.Row="3" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <!-- Document Structure -->
                                <DataTemplate DataType="{x:Type Svg:SvgDefinitionList}">
                                    <Grid Margin="2">
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgDescription}">
                                    <Grid Margin="2">
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgDocumentMetadata}">
                                    <Grid Margin="2">
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgFragment}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- X -->
                                        <TextBlock Text="X" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding X}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- Y -->
                                        <TextBlock Text="Y" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Y}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- Width -->
                                        <TextBlock Text="Width" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Width}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                        <!-- Height -->
                                        <TextBlock Text="Height" Grid.Row="3" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Height}" Grid.Row="3" Grid.Column="1" Margin="2" />
                                        <!-- ViewBox -->
                                        <TextBlock Text="ViewBox" Grid.Row="4" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding ViewBox}" Grid.Row="4" Grid.Column="1" Margin="2" />
                                        <!-- AspectRatio -->
                                        <TextBlock Text="AspectRatio" Grid.Row="5" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding AspectRatio}" Grid.Row="5" Grid.Column="1" Margin="2" />
                                        <!-- ZoomAndPan -->
                                        <TextBlock Text="ZoomAndPan" Grid.Row="6" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding ZoomAndPan}" Grid.Row="6" Grid.Column="1" Margin="2" />
                                        <!-- Version -->
                                        <TextBlock Text="Version" Grid.Row="7" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Version}" Grid.Row="7" Grid.Column="1" Margin="2" />
                                        <!-- BaseProfile -->
                                        <TextBlock Text="BaseProfile" Grid.Row="8" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding BaseProfile}" Grid.Row="8" Grid.Column="1" Margin="2" />
                                        <!-- ContentScriptType -->
                                        <TextBlock Text="ContentScriptType" Grid.Row="9" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding ContentScriptType}" Grid.Row="9" Grid.Column="1" Margin="2" />
                                        <!-- ContentStyleType -->
                                        <TextBlock Text="ContentStyleType" Grid.Row="10" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding ContentStyleType}" Grid.Row="10" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgGroup}">
                                    <Grid Margin="2">
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgImage}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- AspectRatio -->
                                        <TextBlock Text="AspectRatio" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding AspectRatio}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- X -->
                                        <TextBlock Text="X" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding X}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- Y -->
                                        <TextBlock Text="Y" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Y}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                        <!-- Width -->
                                        <TextBlock Text="Width" Grid.Row="3" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Width}" Grid.Row="3" Grid.Column="1" Margin="2" />
                                        <!-- Height -->
                                        <TextBlock Text="Height" Grid.Row="4" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Height}" Grid.Row="4" Grid.Column="1" Margin="2" />
                                        <!-- Href -->
                                        <TextBlock Text="Href" Grid.Row="5" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Href}" Grid.Row="5" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgSwitch}">
                                    <Grid Margin="2">
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgSymbol}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- ViewBox -->
                                        <TextBlock Text="ViewBox" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding ViewBox}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- AspectRatio -->
                                        <TextBlock Text="AspectRatio" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding AspectRatio}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgTitle}">
                                    <Grid Margin="2">
                                    </Grid>
                                </DataTemplate>
                                <DataTemplate DataType="{x:Type Svg:SvgUse}">
                                    <Grid Margin="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <!-- X -->
                                        <TextBlock Text="X" Grid.Row="0" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding X}" Grid.Row="0" Grid.Column="1" Margin="2" />
                                        <!-- Y -->
                                        <TextBlock Text="Y" Grid.Row="1" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Y}" Grid.Row="1" Grid.Column="1" Margin="2" />
                                        <!-- Width -->
                                        <TextBlock Text="Width" Grid.Row="2" Grid.Column="0" Margin="2" />
                                        <TextBox Text="{Binding Width}" Grid.Row="2" Grid.Column="1" Margin="2" />
                                        <!-- Height -->
                                        <TextBlock Text="Height" Grid.Row="3" Grid.Column="0" Margin="2" />
